#include<stdio.h>
int fibonacci(int n)
{
    if(n==0)
        return 0;
    else if (n==1)
        return 1;
    return fibonacci(n-1)+fibonacci(n-2);
}
int main()
{
    int n;
    printf("Enter position : ");
    scanf("%d",&n);
    printf("%d th number of fibonacci Number : %d\n",n,fibonacci(n));

}
